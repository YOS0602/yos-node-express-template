{
  "name": "yos-node-express-template",
  "version": "1.0.2",
  "description": "node.js express template",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn src/app.ts",
    "prebuild": "node -e 'fs.rmSync(`dist`, {recursive:true, force:true})'",
    "build": "tsc --project tsconfig.build.json",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "test": "jest --maxWorkers=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YOS0602/yos-node-express-template.git"
  },
  "author": "YOS0602 <yoshitaka.t.biz@gmail.com> (https://github.com/YOS0602)",
  "license": "",
  "bugs": {
    "url": "https://github.com/YOS0602/yos-node-express-template/issues"
  },
  "homepage": "https://github.com/YOS0602/yos-node-express-template#readme",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.1",
    "@types/sinon-express-mock": "^1.3.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "sinon-express-mock": "^2.2.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "volta": {
    "node": "18.12.1"
  }
}
